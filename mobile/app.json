{
  "expo": {
    "name": "UTCD Forms",
    "slug": "utcd-forms",
    "owner": "utcd-forms",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "utcd-forms",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.utcd-forms.mobile",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show you the nearest forms."
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.utcdforms.mobile"
    },
    "web": {
      "favicon": "./assets/splash-icon-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "73a572e5-bb66-480b-828d-e25e6f5a1f62"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "newArchitecture": false,
      "bridgeless": true
    },

    "plugins": [
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ]
    ]
  }
}
